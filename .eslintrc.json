{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": "eslint:recommended",
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "rules": {
    "no-unused-vars": "warn",
    "no-console": "off",
    "no-undef": "warn"
  },
  "globals": {
    "window": "readonly",
    "document": "readonly",
    "console": "readonly",
    "fetch": "readonly",
    "FormData": "readonly",
    "localStorage": "readonly",
    "setTimeout": "readonly",
    "clearTimeout": "readonly"
  }
}
